<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:background="@color/beyaz"
        android:layout_height="match_parent"
        tools:context=".Account.ProfileSettingActivity">

    <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:id="@+id/profileSttingsRoot"
            android:layout_height="match_parent">

        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/ic_back"
                android:id="@+id/back"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="16dp"
                android:layout_marginStart="16dp"/>

        <TextView
                android:text="Ayarlar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/ayarlar"
                app:layout_constraintStart_toEndOf="@+id/back"
                android:layout_marginLeft="24dp"
                android:layout_marginStart="24dp"
                tools:ignore="MissingConstraints"
                app:layout_constraintBottom_toBottomOf="@+id/back"
                app:layout_constraintTop_toTopOf="@+id/back"
                android:textSize="18sp"/>

        <View
                android:layout_width="match_parent"
                android:id="@+id/golgelik"
                android:background="@color/golgelik"
                android:layout_height="2dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/ayarlar">


        </View>

        <TextView
                android:text="Hesap Ayarları"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/hesapAyarlarıText"
                android:textStyle="bold"
                tools:ignore="MissingConstraints"
                app:layout_constraintTop_toBottomOf="@+id/golgelik"
                android:layout_marginStart="16dp"
                app:layout_constraintStart_toStartOf="@+id/golgelik"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="16dp"
                android:textSize="16sp"/>

        <TextView
                android:text="Profili Düzenle"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:id="@+id/profiliDüzenleText"
                tools:ignore="MissingConstraints"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="16dp"
                android:textStyle="bold"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                app:layout_constraintTop_toBottomOf="@+id/hesapAyarlarıText"
                android:layout_marginEnd="8dp"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginRight="8dp"/>
        <TextView
                android:text="Beğendiğin Gönderiler"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:id="@+id/begenilenlerText"
                tools:ignore="MissingConstraints"
                android:layout_marginTop="16dp"
                android:textStyle="bold"
                app:layout_constraintTop_toBottomOf="@+id/profiliDüzenleText"
                android:layout_marginStart="16dp"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="16dp"
                android:layout_marginEnd="8dp"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginRight="8dp"/>

        <TextView
                android:text="Kaydedilenler"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:id="@+id/kaydedilenler"
                tools:ignore="MissingConstraints"
                android:layout_marginTop="16dp"
                android:textStyle="bold"
                app:layout_constraintTop_toBottomOf="@+id/begenilenlerText"
                android:layout_marginStart="16dp"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="16dp"
                android:layout_marginEnd="8dp"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginRight="8dp"/>

        <TextView
                android:text="Gizli Hesap"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:id="@+id/gizliHesap"
                tools:ignore="MissingConstraints"
                android:layout_marginTop="16dp"
                app:layout_constraintTop_toBottomOf="@+id/kaydedilenler"
                android:layout_marginStart="16dp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="16dp"
                android:layout_marginEnd="8dp"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginRight="8dp"/>

        <TextView
                android:text="Arama Geçmişini Sil"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:id="@+id/textView11"
                android:layout_marginTop="24dp"
                android:textColor="@color/mavi"
                app:layout_constraintTop_toBottomOf="@+id/gizliHesap"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="16dp"
                android:layout_marginStart="16dp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp"
                android:layout_marginRight="8dp"/>
        <TextView
                android:text="Çıkış Yap"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:id="@+id/cikisYapText"
                android:layout_marginTop="16dp"
                app:layout_constraintTop_toBottomOf="@+id/textView11"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="16dp"
                android:textStyle="bold"
                android:textColor="@color/mavi"
                android:layout_marginStart="16dp"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp"
                android:layout_marginRight="8dp"/>

        <com.ittianyu.bottomnavigationviewex.BottomNavigationViewEx
                android:layout_width="match_parent"
                android:id="@+id/bottomNavigationView"
                android:layout_height="50dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:menu="@menu/bottom_navigation_menu"
                app:layout_constraintStart_toStartOf="parent">

        </com.ittianyu.bottomnavigationviewex.BottomNavigationViewEx>

        <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:id="@+id/golgelik2"
                android:background="@color/golgelik"
                app:layout_constraintTop_toBottomOf="@+id/homeViewPager"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toTopOf="@+id/bottomNavigationView">

        </View>


    </android.support.constraint.ConstraintLayout>

    <FrameLayout
            android:layout_width="match_parent"
            android:id="@+id/profileSettingContainer"
            android:layout_height="match_parent">



    </FrameLayout>

</android.support.constraint.ConstraintLayout>

